{
	"manifest_version": 3,
	"name": "Skipper",
	"version": "1.0.0",
	"action": {
		"default_title": "Configure Skipper",
		"default_popup": "index.html"
	},
	"permissions": ["tabs", "webRequest", "scripting"],
	"host_permissions": ["https://www.youtube.com/*", "https://music.youtube.com/*"],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
